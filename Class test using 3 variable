class Test {
    public int num = 3;

    public static void main(String[] args) {
        Test ob = new Test(0);  
        ob.get();               
    }

    
    public Test(int n) {
        System.out.println("Constructor called with value: " + n);
    }

    
    public void get() {
        System.out.println("get method called");
    }
}
